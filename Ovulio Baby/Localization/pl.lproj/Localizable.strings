//MARK: Affirmation text
"Affirmation.Categories0.headline0.text" = "Mój organizm jest gotowy do poczęcia.";
"Affirmation.Categories0.headline1.text" = "Miłość i nadzieja towarzyszą mi na drodze do rodzicielstwa.";
"Affirmation.Categories0.headline2.text" = "Każdy dzień zbliża mnie do mojego wytęsknionego dziecka.";
"Affirmation.Categories0.headline3.text" = "Zaufam swojemu ciału i jego naturalnemu rytmowi.";
"Affirmation.Categories0.headline4.text" = "Z każdym oddechem otrzymuję pozytywną energię dla mojego pragnienia posiadania dziecka.";
"Affirmation.Categories0.headline5.text" = "Mój umysł i ciało są w harmonii dla spełnienia mojego marzenia.";
"Affirmation.Categories0.headline6.text" = "Miłość, którą czuję, wspiera moją drogę do rodzicielstwa.";
"Affirmation.Categories0.headline7.text" = "W każdej chwili jestem otwarty na cuda życia.";
"Affirmation.Categories0.headline8.text" = "Manifestuję miłość, szczęście i spełnienie w mojej rodzinie.";
"Affirmation.Categories0.headline9.text" = "Moje pragnienie posiadania dzieci zostanie spełnione w odpowiednim czasie.";
"Affirmation.Categories0.headline10.text" = "Potrafię przyjąć pomoc i otrzymać wsparcie na swojej drodze.";
"Affirmation.Categories0.headline11.text" = "Jestem gotowy na piękną ciążę.";
"Affirmation.Categories0.headline12.text" = "Dobrze dbam o siebie.";
"Affirmation.Categories0.headline13.text" = "Zaufam swojej intuicji.";
"Affirmation.Categories0.headline14.text" = "Moja kolej nadejdzie.";
"Affirmation.Categories0.headline15.text" = "Jestem zdrowy/zdrowa.";
"Affirmation.Categories0.headline16.text" = "Jestem wdzięczny/wdzięczna za wszystko, co mam.";
"Affirmation.Categories0.headline17.text" = "Jestem gotowy/gotowa na poczęcie dziecka.";
"Affirmation.Categories0.headline18.text" = "Mam najlepsze warunki dla dziecka.";
"Affirmation.Categories0.headline19.text" = "Mój organizm jest płodny.";
"Affirmation.Categories0.headline20.text" = "Będę w ciąży.";
"Affirmation.Categories0.headline21.text" = "Moje żeńskie ciało jest silne.";
"Affirmation.Categories0.headline22.text" = "Jestem otwarty/otwarta na nowe doświadczenia.";
"Affirmation.Categories0.headline23.text" = "Moje ciało jest silne i zdrowe: Wierzę, że moje ciało jest silne i zdrowe, gotowe przyjąć cud ciąży.";
"Affirmation.Categories0.headline24.text" = "Każdy cykl zbliża mnie do celu: Każdy miesiączkowy cykl to kolejny krok na drodze do mojej ciąży. Każdy cykl przybliża mnie do mojego celu.";
"Affirmation.Categories0.headline25.text" = "Jestem cierpliwy/cierpliwa i ufam procesowi: Dobre rzeczy potrzebują czasu. Ćwiczę cierpliwość i ufam, że wszystko stanie się we właściwym czasie.";
"Affirmation.Categories0.headline26.text" = "Dbam o swoje zdrowie emocjonalne: Troszcząc się o swoje zdrowie emocjonalne, tworzę pozytywną przestrzeń dla optymalnego funkcjonowania mojego ciała.";
"Affirmation.Categories0.headline27.text" = "Miłość i wsparcie mnie otaczają: Jestem otoczony/a miłością i wsparciem, czy to ze strony partnera, rodziny czy przyjaciół. Razem przechodzimy przez tę podróż.";
"Affirmation.Categories0.headline28.text" = "Wizualizuję pozytywną przyszłość: Wizualizuję moje życie jako matka i radość, jaką przyniesie. Ta pozytywna wizja wzmacnia moją motywację.";
"Affirmation.Categories0.headline29.text" = "Dbam o swoje ciało: Poprzez zdrowe odżywianie, regularne ćwiczenia i odpowiednią ilość snu dbam o swoje ciało i tworzę optymalne warunki dla ciąży.";
"Affirmation.Categories0.headline30.text" = "Ustalam realne oczekiwania: Rozumiem, że zajście w ciążę wymaga czasu i zaangażowania. Ustalam realne oczekiwania i cieszę się z każdego postępu.";
"Affirmation.Categories0.headline31.text" = "Moja miłość i zaangażowanie są cenne: Moje pragnienie założenia rodziny pokazuje moją miłość i zaangażowanie. Te cechy będą wspierać wzbogacające rodzicielstwo.";
"Affirmation.Categories0.headline32.text" = "Każdy dzień to nowa szansa: Każdy dzień niesie nowe możliwości nadziei i sukcesu. Witam każdy dzień jako szansę na pozytywną zmianę.";
"Affirmation.Categories0.headline33.text" = "Łączę się z moim partnerem: Podróż do zajścia w ciążę to wspólna podróż. Łączenie się z moim partnerem wzmacnia nasze wzajemne wsparcie.";
"Affirmation.Categories0.headline34.text" = "Odpuszczam i się relaksuję: Odpuszczając i relaksując się, tworzę przestrzeń dla naturalnych procesów w moim ciele.";
"Affirmation.Categories0.headline35.text" = "Kocham i akceptuję moje ciało: Bez względu na wyzwania, kocham i akceptuję moje ciało. Ta miłość przyczynia się do pozytywnego podejścia.";
"Affirmation.Categories0.headline36.text" = "Każdy wysiłek jest wartościowy: Każdy krok, jaki podejmuję, czy to poprzez zdrowe nawyki czy wsparcie medyczne, to wartościowy wkład w mój cel.";
"Affirmation.Categories0.headline37.text" = "Wierzę w swoje zdolności: Wierzę w moje zdolności jako przyszła mama. To pozytywne podejście wzmacnia moją wiarę w sukces.";
"Affirmation.Categories0.headline38.text" = "Rozpoznaję stres i odpuszczam - robię miejsce dla zrelaksowanego pragnienia posiadania dzieci.";
"Affirmation.Categories0.headline39.text" = "Mój umysł i ciało znajdują pokój na drodze do szczęścia z dzieckiem.";
"Affirmation.Categories0.headline40.text" = "Ufam procesowi i redukuję stres dla harmonijnego poczęcia.";
"Affirmation.Categories0.headline41.text" = "Relaksacja otwiera drogę dla mojego pragnienia posiadania dzieci.";
"Affirmation.Categories0.headline42.text" = "Stres znika, gdy z miłością wyruszam na moją drogę.";
"Affirmation.Categories0.headline43.text" = "Wydycham stres i wdycham poczęcie.";
"Affirmation.Categories0.headline44.text" = "Moje spokojne nastawienie wspomaga płodność.";
"Affirmation.Categories0.headline45.text" = "Każdy oddech przynosi spokój i pewność siebie.";
"Affirmation.Categories0.headline46.text" = "W spokoju znajduję siłę dla mojego pragnienia posiadania dzieci.";
"Affirmation.Categories0.headline47.text" = "Codziennie coraz bardziej odpuszczam stres.";


"Affirmation.Categories1.headline0.text" = "Rozpoznaję moje lęki i miłościwie je odpuszczam - w drodze do mojego dziecka.";
"Affirmation.Categories1.headline1.text" = "Moje zaufanie do mojego ciała przewyższa wszelkie obawy, jakie mam dotyczące posiadania dzieci.";
"Affirmation.Categories1.headline2.text" = "Każdy oddech umacnia mnie w obliczu moich zmartwień i obaw.";
"Affirmation.Categories1.headline3.text" = "Lęki ustępują, gdy skupiam się na mojej miłości do mojego dziecka.";
"Affirmation.Categories1.headline4.text" = "Moje myśli są pozytywne, lęki nie mają miejsca w moim pragnieniu posiadania dziecka.";
"Affirmation.Categories1.headline5.text" = "Jestem silny/silna i odważny/odważna, lęki nie mają wpływu na moją drogę.";
"Affirmation.Categories1.headline6.text" = "Znajduję pokój w spokoju i pokonuję lęki.";
"Affirmation.Categories1.headline7.text" = "Nadzieja na moje dziecko przysłania wszelkie lęki.";
"Affirmation.Categories1.headline8.text" = "Moje serce jest wolne od lęków - miłość kieruje moje pragnienie posiadania dziecka.";
"Affirmation.Categories1.headline9.text" = "Lęki rozpuszczają się, gdy się przygotowuję do mojego dziecka.";


"Affirmation.Categories2.headline0.text" = "Jestem dobry/dobra tak, jak jestem.";
"Affirmation.Categories2.headline1.text" = "W każdym oddechu wybieram pozytywność i pokój.";
"Affirmation.Categories2.headline2.text" = "Każdy dzień to nowa okazja do wzrostu i szczęścia.";
"Affirmation.Categories2.headline3.text" = "Miłość i światło płyną przez moje życie.";
"Affirmation.Categories2.headline4.text" = "Przyciągam pozytywne energie i możliwości.";
"Affirmation.Categories2.headline5.text" = "Moje serce jest otwarte na radość i wdzięczność.";
"Affirmation.Categories2.headline6.text" = "Kształtuję swoje życie zgodnie z moimi marzeniami i wartościami.";
"Affirmation.Categories2.headline7.text" = "Podążam ścieżką mojego przeznaczenia z pewnością siebie.";
"Affirmation.Categories2.headline8.text" = "Jestem architektem mojego szczęścia i świadomie je kształtuję.";
"Affirmation.Categories2.headline9.text" = "Moje wnętrze promieniuje pozytywnością, miłością i pokojem.";
"Affirmation.Categories2.headline10.text" = "Promieniuję pozytywną energią i przyciągam dobre rzeczy.";
"Affirmation.Categories2.headline11.text" = "Każdy dzień niesie nowe możliwości i pozytywne perspektywy.";
"Affirmation.Categories2.headline12.text" = "Z każdym oddechem wzmacniam swój wewnętrzny pokój i siłę.";
"Affirmation.Categories2.headline13.text" = "Miłość i światło towarzyszą mi na mojej drodze życiowej.";
"Affirmation.Categories2.headline14.text" = "Jestem otwarty/a na wzrost, szczęście i pozytywne zmiany.";
"Affirmation.Categories2.headline15.text" = "Moje serce jest pełne miłości i wdzięczności.";
"Affirmation.Categories2.headline16.text" = "Kształtuję swoje życie świadomie i pozytywnie.";
"Affirmation.Categories2.headline17.text" = "Optymizm jest moim stałym towarzyszem.";
"Affirmation.Categories2.headline18.text" = "Jestem twórcą mojej własnej pozytywnej rzeczywistości.";
"Affirmation.Categories2.headline19.text" = "Moje życie jest wypełnione radością, miłością i pozytywnymi doświadczeniami.";
"Affirmation.Categories2.headline20.text" = "Myśl pozytywnie, żyj pozytywnie - każdego dnia na nowo.";
"Affirmation.Categories2.headline21.text" = "Jestem architektem mojego szczęścia i świadomie je kształtuję.";
"Affirmation.Categories2.headline22.text" = "Radość życia płynie przez mnie, dzień po dniu.";
"Affirmation.Categories2.headline23.text" = "Znajduję powody do wdzięczności w każdej chwili.";
"Affirmation.Categories2.headline24.text" = "Moje myśli tworzą świat pełen pozytywności.";
"Affirmation.Categories2.headline25.text" = "Jestem magnesem dla dobrej energii i chwil szczęścia.";
"Affirmation.Categories2.headline26.text" = "Kształtuję swoje życie pełne optymizmu z pewnością siebie.";
"Affirmation.Categories2.headline27.text" = "Znajduję głęboką radość w prostocie życia.";
"Affirmation.Categories2.headline28.text" = "Pozytywność jest moim źródłem siły, które mnie wspiera.";
"Affirmation.Categories2.headline29.text" = "Moje serce bije w rytmie pozytywnych możliwości.";
"Affirmation.Categories2.headline30.text" = "Jestem twórcą mojej rzeczywistości. Dziś wybieram miłość, radość i pokój.";
"Affirmation.Categories2.headline31.text" = "Każdy oddech odświeża mój duch. Jestem gotowy/gotowa na obfitość, którą przynosi mi ten dzień.";
"Affirmation.Categories2.headline32.text" = "Z każdym nowym dniem moja siła i mądrość rosną.";
"Affirmation.Categories2.headline33.text" = "Ufam w proces życia i pozwalam sobie na to, co nie jest pod moją kontrolą.";
"Affirmation.Categories2.headline34.text" = "Dziś otwieram swoje serce na nowe możliwości i pozytywne spotkania.";
"Affirmation.Categories2.headline35.text" = "Głęboka serenada spoczywa we mnie. Jestem w harmonii z moim wewnętrznym spokojem.";
"Affirmation.Categories2.headline36.text" = "Jestem magnesem dla pozytywnych energii. Dobre rzeczy przyciągają dobre rzeczy.";
"Affirmation.Categories2.headline37.text" = "Mój dzień zaczyna się od miłości i kończy wdzięcznością.";
"Affirmation.Categories2.headline38.text" = "Jestem architektem moich marzeń. Dziś buduję moje szczęście.";
"Affirmation.Categories2.headline39.text" = "Moje ciało jest zdrowe, mój umysł jest klarowny. Jestem gotowy/gotowa na przygody tego dnia.";


"Affirmation.Categories3.headline0.text" = "Silni razem: Znajdujemy miłość i wsparcie w naszym partnerstwie.";
"Affirmation.Categories3.headline1.text" = "Każdego dnia wzmacniamy naszą miłość i więź z sobą.";
"Affirmation.Categories3.headline2.text" = "Zaufanie i szacunek rosną w naszym związku.";
"Affirmation.Categories3.headline3.text" = "Nasza miłość pokonuje każde wyzwanie razem.";
"Affirmation.Categories3.headline4.text" = "Razem tworzymy przestrzeń dla radości i wzajemnego rozwoju.";
"Affirmation.Categories3.headline5.text" = "Miłość jest naszym kotwicą we wszystkich sytuacjach.";
"Affirmation.Categories3.headline6.text" = "Nasze partnerstwo cechuje zrozumienie i komunikacja.";
"Affirmation.Categories3.headline7.text" = "Znajdujemy siłę i szczęście w naszej miłości.";
"Affirmation.Categories3.headline8.text" = "Idziemy razem za rękę przez wzloty i upadki, umacniani przez naszą miłość.";
"Affirmation.Categories3.headline9.text" = "Nasz związek jest miejscem miłości i wspólnych marzeń.";
"Affirmation.Categories3.headline10.text" = "Nasza miłość rośnie z każdą chwilą, którą spędzamy razem.";
"Affirmation.Categories3.headline11.text" = "W naszym partnerstwie jest miejsce na zaufanie i śmiech.";
"Affirmation.Categories3.headline12.text" = "Razem tworzymy miłość, która jest silna i niezachwiana.";
"Affirmation.Categories3.headline13.text" = "Doskonale się uzupełniamy, nasze partnerstwo jest pełne harmonii.";
"Affirmation.Categories3.headline14.text" = "W naszym związku jest miejsce na miłość, wolność i szacunek.";
"Affirmation.Categories3.headline15.text" = "Nasze połączenie jest źródłem radości i szczęścia.";
"Affirmation.Categories3.headline16.text" = "Miłość i akceptacja charakteryzują nasze partnerstwo.";
"Affirmation.Categories3.headline17.text" = "Razem pokonujemy każde wyzwanie, umacniani przez naszą miłość.";
"Affirmation.Categories3.headline18.text" = "Znajdujemy pokój i bezpieczeństwo w naszej miłości.";
"Affirmation.Categories3.headline19.text" = "Nasze partnerstwo to podróż pełna miłości i wspólnych doświadczeń.";


